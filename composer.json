{
    "name": "afex/yii2-craft",
    "description": "Yii2 project template",
    "type": "project",
    "authors": [
        {
            "name": "Leorne",
            "email": "leorneq@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
